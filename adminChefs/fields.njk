<div class="item">
  <div>Avatar URL</div>
    <div>
      <input
      type="url"
      name="avatar_url"
      placeholder="http://"
      value="{{chef.avatar_url}}">
    </div>
</div>

<div class="item">
  <div>Nome do Chef</div>
  <div>
    <input
    type="text"
    name="name"
    placeholder="Digite o nome do Chef"
    value="{{chef.name}}">
  </div>
</div>

<button class="save" type="submit">Salvar</button>
